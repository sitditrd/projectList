<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="stopover">
	<select id="searchobj" parameterType="int" resultType="dto.StopoverDTO">
		select * from stopover where stopover_code = #{num}
	</select>
</mapper>
